package com.esm.esmzone;

class StateProvince {

	Date dateCreated
	Date lastUpdated
	
	String name
	String abbreviation
	
	static constraints = {
		name blank:false, nullable:false
		abbreviation blank:false, nullable:false
	}
	
	static mapping = {
		sort 'name'
	}

	String toString(){ 
		return name
	}		
	
}
